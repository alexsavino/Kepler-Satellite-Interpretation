   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The root:  4054.69\n",
      "The function using this root:  0.0001744966\n"
     ]
    }
   ],
   "source": [
    "import math as m\n",
    "import numpy as np\n",
    "\n",
    "def luminosity(T_eff):\n",
    "    luminosity = (T_eff / 5780)**4\n",
    "    return luminosity\n",
    "\n",
    "def S_eff(T_eff):\n",
    "    T_star = T_eff - 5780\n",
    "    S_eff_sun = 1.104\n",
    "    a = (8.177 * 10**(-5))\n",
    "    b = (1.706 * 10**(-9))\n",
    "    S = (S_eff_sun + (a*T_star) + (b*(T_star)**2))\n",
    "    return S\n",
    "\n",
    "def habitable(T_eff):\n",
    "    diameter = math.sqrt(luminosity(T_eff)/S_eff(T_eff))\n",
    "    return diameter\n",
    "\n",
    "def function(T_eff):\n",
    "    function_value = habitable(T_eff) - 0.5\n",
    "    return function_value\n",
    "\n",
    "\n",
    "T_eff = np.arange(1000,7000,1)\n",
    "a = []\n",
    "for t in T_eff:\n",
    "    f = function(t)\n",
    "    a.append(f)\n",
    "\n",
    "\n",
    "def bisection(f, a, b, tol):\n",
    "    m = (a + b)/2\n",
    "    if np.abs(f(m)) < tol:\n",
    "        return m\n",
    "    elif np.sign(f(a)) == np.sign(f(m)):\n",
    "        # For the right side ...\n",
    "        return bisection(f, m, b, tol)\n",
    "    elif np.sign(f(b)) == np.sign(f(m)):\n",
    "        # For the left side ...\n",
    "        return bisection(f, a, m, tol)\n",
    "\n",
    "\n",
    "f = lambda x : function(x)\n",
    "\n",
    "# For the final result ...\n",
    "root = bisection(f, 2000, 6000, 0.0005)\n",
    "print(\"The root: \", round(root,2))\n",
    "print(\"The function using this root: \", round(f(root),10))"
   ]
  }

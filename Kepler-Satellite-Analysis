   "source": [
    "import math as math\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "\n",
    "def simpson_int_arr(f, x):\n",
    "    h = (x[1] - x[0])\n",
    "    fx = f(x)                   \n",
    "    fx_half = f(x + 0.5*h)          \n",
    "    fx_start = f(x[ 0])                \n",
    "    fx_end = f(x[-1] +     h)      \n",
    "    result = (fx_start + 2*(fx + 2*fx_half).sum() + fx_end) * h / 6.0\n",
    "    # Return statement ... \n",
    "    return result \n",
    "\n",
    "def delta_arr(p,r,z):\n",
    "    if np.isscalar(r):\n",
    "        nr = 1\n",
    "        r  = np.array([r])\n",
    "    else:\n",
    "        nr = len(r)\n",
    "    result = (np.zeros(nr)-99)                       \n",
    "    result[ np.where(r + z <= p)] = 1\n",
    "    result[np.concatenate((np.where(r >= z + p)[0], np.where(r <= z - p)[0]))] = 0.0  \n",
    "    index = np.where(result == -99)[0]\n",
    "    result[index] = (np.arccos((z*z - p*p + r[index]**2) / (2.0 * z * r[index] + (1e-14))) / np.pi)\n",
    "    return result\n",
    "\n",
    "\n",
    "def limb_dark(r):\n",
    "    u = (1 - r**2)**(1/2)\n",
    "    limb_dark = 1 - (1 - u**(3/2))\n",
    "    return limb_dark\n",
    "\n",
    "\n",
    "def flux_arr(p, z, int_function, limb_function, N):\n",
    "   \n",
    "    def numerator(r, p=p, z=z):  \n",
    "        return limb_function(r) * (1.0 - delta_arr(p, r, z)) * 2.0 * r\n",
    "   \n",
    "    def denominator (r):\n",
    "        return limb_function(r) * 2.0 * r\n",
    "\n",
    "    lower = 0.0\n",
    "    upper = 1.0\n",
    "    r = np.arange(lower, upper, (upper - lower)/N)\n",
    "    \n",
    "    top = int_function(numerator,   r)\n",
    "    bot = int_function(denominator, r)\n",
    "    \n",
    "    return top/bot\n",
    "\n",
    "\n",
    "# Code from Assignment 1 ...\n",
    "def wavelength(p,z):\n",
    "    z = abs(z)\n",
    "    # below are all the piecewise possibilities\n",
    "    if (1+p)<z:\n",
    "        wavelength_value = 0.00\n",
    "    elif 0<=((4*z**2 - (1 + z**2 - p**2)**2)/4):\n",
    "        return ((k_0(p,z)*p**2 + k_1(p,z) - np.sqrt((4*z**2 - (1 + z**2 - p**2)**2)/4))/np.pi)\n",
    "    elif (z)<=(1-p):\n",
    "        wavelength_value = (p**2)\n",
    "    elif (z)<=(p-1):\n",
    "        wavelength_value = 1.00\n",
    "    # now to send the result to flux(p,z)\n",
    "    # '{:.2f}%'.format(wavelength_value)\n",
    "    return wavelength_value\n",
    "\n",
    "def flux(p,z):\n",
    "    return (\"The result is: \" + str(1.00 - wavelength(p,z)))\n",
    "    \n",
    "    \n",
    "p = 0.2\n",
    "z = 0.9\n",
    "f_true = FluxRatio(p,z)\n",
    "print('nx,    f_int,    f_true,     relative error')\n",
    "for i in range(1,5):\n",
    "    f_int = flux_arr(p, z, simpson_int_arr, limb, 10**i)\n",
    "    print(10**i, f_int, f_true, abs(f_int - f_true)/f_true)\n",
    "    \n",
    "\n",
    "# To begin the plotting process ...\n",
    "p = 0.1\n",
    "z = np.arange(-1, 1, 0.001)\n",
    "\n",
    "c = np.zeros(4)\n",
    "for ic in range(5):\n",
    "    if ic > 0:\n",
    "        c[ic-1] = 1\n",
    "    f_int = []\n",
    "    for i in range(len(z)):\n",
    "        f_int.append(flux_arr(p, abs(z[i]), simpson_int_arr, limb_dark, 1000))\n",
    "    plt.plot(z, f_int)\n",
    "    c = np.zeros(4)\n",
    "\n",
    "# Final plotting details ...\n",
    "plt.title(\"Normalized Flux vs Normalized Z\", fontsize=20)\n",
    "plt.xlabel(\"Normalized Z\", fontsize=12)\n",
    "plt.ylabel(\"Normalized Flux\", fontsize=12)"
   ]

   "source": [
    "# I worked closely with Leah to develop this section of the code ...\n",
    "import numpy as np\n",
    "import math as math\n",
    "%matplotlib inline\n",
    "\n",
    "\n",
    "# To define the square_root factor ...\n",
    "def sqrt_factor(p,z):\n",
    "    sqrt_factor_1 = (4*z*z)\n",
    "    sqrt_factor_2 = ((1 + z*z - p*p)**2)\n",
    "    if (sqrt_factor_1 > sqrt_factor_2):\n",
    "        sqrt_factor = math.sqrt(0.25*(sqrt_factor_1 - sqrt_factor_2))\n",
    "    elif math.fabs((sqrt_factor_2 - sqrt_factor_1)/sqrt_factor_1) < 1e-5:\n",
    "        sqrt_factor = 0\n",
    "    else:\n",
    "        print('p = ', p, 'z = ', z, 'sqrt_factor_1 = ', sqrt_factor_1, 'sqrt_factor_2 =', sqrt_factor_2)       \n",
    "    return sqrt_factor\n",
    "\n",
    "\n",
    "# Code from Problem 1 ... \n",
    "def wavelength(p,z):\n",
    "    z = abs(z)\n",
    "    # Below are all the piecewise possibilities\n",
    "    if (1+p)<z:\n",
    "        wavelength_value = 0.00\n",
    "    elif (math.fabs(1.0-p) < z) and (z <= (1.0+p)):\n",
    "        sqrt_fact = sqrt_factor(p,z)       \n",
    "        arg_k_0 = (p*p + z*z - 1.0)/ (2.0*p*z)\n",
    "        arg_k_1 = (1 - p*p + z*z) / (2.0*z) \n",
    "        k_0 = math.acos(arg_k_0)\n",
    "        k_1 = math.acos(arg_k_1)\n",
    "        return (1.0/math.pi) * (p*p*k_0 + k_1 - sqrt_fact)    \n",
    "    elif (z)<=(1-p):\n",
    "        wavelength_value = (p**2)\n",
    "    elif (z)<=(p-1):\n",
    "        wavelength_value = 1.00    \n",
    "    return wavelength_value\n",
    "\n",
    "\n",
    "nz = round((1.2*2.0 / 0.05) + 1.0) ; print(nz, int(nz))\n",
    "z = np.zeros(int(nz))   \n",
    "i = np.arange(nz)               \n",
    "z = i*0.05-1.2    \n",
    "\n",
    "\n",
    "F = []\n",
    "for e in eclipse_z: \n",
    "    hello = (1 - wavelength(0.58, abs(e)))\n",
    "    F.append(hello)\n",
    "   # print(round(e,2), hello)\n",
    "    i += 1\n",
    "    \n",
    "    \n",
    "# Plotting details ...\n",
    "fig = plt.figure(figsize=(8,6))\n",
    "plt.plot(eclipse_z,F)\n",
    "plt.title(\"Normalized Flux vs Normalized Distance\", fontsize=20)\n",
    "plt.xlabel('Normalized Distance $z$', fontsize=12)\n",
    "plt.ylabel('Normalized Flux $F$', fontsize=12)\n",
    "plt.xlim(-0.75, 0.75)\n",
    "plt.show()"
   ]
